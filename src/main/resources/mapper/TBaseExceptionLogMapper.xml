<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapabc.signal.dao.mapper.TBaseExceptionLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mapabc.signal.common.component.ExceptionLog">
        <id column="id" property="id"/>
        <result column="uri" property="uri"/>
        <result column="system" property="system"/>
        <result column="operator_ip" property="operatorIp"/>
        <result column="host_name" property="hostName"/>
        <result column="brower_message" property="browerMessage"/>
        <result column="description" property="description"/>
        <result column="detail" property="detail"/>
        <result column="create_time" property="createTime"/>
    </resultMap>

    <!--基本字段列表-->
    <sql id="Base_Column_List">
        id,
        uri,
        system,
        operator_ip,
        host_name,
        brower_message,
        description,
        detail,
        create_time
    </sql>

</mapper>
